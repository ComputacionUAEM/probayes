set xlabel "VT"
set ylabel " P(VT)"

set xrange [-10:10]
set yrange [0:]
set nokey
set samples 19
#erase the four following lines if you don't want filled histograms
set style data histogram
set style histogram cluster gap 1
set style fill solid border -1
set boxwidth 0.9
set title "plTabulatedDistribution(VT)"

plot "archivo.data" with boxes
pause -1 "Hit return to continue" #Uncoment the following lines to generate a postscript file
